pipeline {
  agent any

  stages {
    stage ('init') {
      steps {
        load('env-dev.groovy')
        sh("echo Dealing with ${env.repoUrl} in ${env.env}")
      }
    }
    stage ('secret stuff') {
      environment {
        SOMECREDS = credentials("${env.SOMECREDSID}")
      }
      steps {
        sh("echo ${env.SOMECREDS_USR} | base64")
        sh("echo ${env.SOMECREDS_PSW} | base64")
      }
    }
  }
}
